# _cleansrc _unpack _cdsrc _patch _conf _build _check
# _destin _postin _cleanpkg _checkin _makepkg
unset _patch _conf _build _check _postin _makepkg

dep="python"

_destin() {
python setup.py install \
  --prefix=/usr \
  --standard-lib \
  --optimize=1 \
  --install-data=/usr/share \
  --root=$D
}
